{% extends 'base2.jinja2' %}

{% block head %}


<style>
*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
a, a:visited{
    color: black;
    text-decoration: none;
    cursor: pointer;
}
body{
    padding: 0 125px;
    font-family: 'Bai Jamjuree', sans-serif;
    padding-top: 10px;
}
.heroButton{
    border: 1px solid #292929;
    border-radius: 5px;
    font-size: 1.1em;
    padding: 4px 12px;
    text-transform: lowercase;
    display: inline-block;
}
.createButton{
    background-color: #D2EA9B;
    margin-right: 30px;
}
.joinButton:hover{
    background-color: #D2EA9B;
}
.nogroupMain{
    height: 100vh;
    position: absolute;
    top: 0;
    width: calc(100% - 250px);
    display: flex;
    justify-content: center;
    align-items: center;
    background-image: url("../static/icon-background.svg");
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center center;
    z-index: -1;
}
.nogroupText{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.nogroupMessage{
    font-size: 1.2em;
    font-weight: bold;
    margin-bottom: 20px;
}
.nogroupMain .row1{
    margin-bottom: 50px;
}
</style>
{% endblock %}

<body>
{% block content %}

<p>Name: {{ userdata['firstName'] }} {{ userdata['lastName'] }} </p>
<p>Email: {{ userdata['email'] }}</p>
<p>Number: {{ userdata['phoneNumber'] }}</p>







{% endblock %}
</body>
